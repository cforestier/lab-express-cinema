<a href="/movies">Go Back</a>
<h1>{{movie.title}}</h1>
<hr>
<img src="{{movie.image}}" alt="{{title}}">
<p>Director: {{movie.director}}</p>
<p>stars</p>
<ul>
    {{#each movie.stars}}
    <li>
    {{this}}
    </li>
    {{/each}}
</ul>
<p>{{movie.description}}</p>
{{#each movie.showtimes}}
<p>{{this}}|</p>
{{/each}}